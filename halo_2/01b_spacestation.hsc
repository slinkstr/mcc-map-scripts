(script startup void mission_spacestation
    (begin
        (if (= (player_count) 0) 
            (sleep_forever))
        (cinematic_snap_to_white)
        (save_cutscene)
        (if (cinematic_skip_start) 
            (begin
                (cinematic_stash_players)
                (x02)
                (cinematic_unstash_players)
            )
        )
        (cinematic_skip_stop)
        (object_type_predict "scenarios\objects\solo\spacestation\ss_tram_car\ss_tram_car")
        (camera_control false)
        (sleep 1)
        (ui_error_ok)
        (sleep 1)
        (mission_start)
        (mission_setup)
        (load_bsp0a)
        (wake flavor_mission)
        (wake 1st_mission)
