        (wake atr1_mission)
        (wake trm1_mission)
        (wake dck_mission)
        (sleep_until (= (version) 3) 1)
        (load_bsp3)
        (wake elv_mission)
        (wake lvr_mission)
        (gun_mission)
        (playtest_mission)
        (save_cutscene)
        (cinematic_fade_to_white)
        (device_set_power "gun_loader" 0.0)
        (device_set_power "macgun" 0.0)
        (sleep 45)
        (if (cinematic_skip_start) 
            (begin
                (cinematic_stash_players)
                (c01_outro)
                (cinematic_unstash_players)
            )
        )
        (cinematic_skip_stop)
        (game_revert)
    )
)

