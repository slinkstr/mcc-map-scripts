(script static void 1st_malta
    (begin
        (sound_looping_stop_immediately "scenarios\solo\01b_spacestation\01b_music\01b_01" none 1.0)
        (sleep 30)
        (print "how's it going, malta?")
        (sound_impulse_time "sound\dialog\levels\01_spacestation\mission\l01_0080_jon" (ai_get_object "1st_hum_johnson") 1.0 "radio_default")
        (sleep (sound_impulse_start_3d "sound\dialog\levels\01_spacestation\mission\l01_0080_jon"))
        (sleep 7)
        (print "standby...")
        (sound_impulse_time "sound\dialog\levels\01_spacestation\mission\l01_0090_mm1" none 1.0 "radio_default")
        (sleep (sound_impulse_start_3d "sound\dialog\levels\01_spacestation\mission\l01_0090_mm1"))
        (sound_impulse_time "sound\ambience\spacestation\docking_clangs\docking_mono" none 1.0 "radio_default")
        (sleep 5)
        (print "they're latched!")
        (sound_impulse_time "sound\dialog\levels\01_spacestation\mission\l01_0100_mm1" none 1.0 "radio_default")
        (sleep (sound_impulse_start_3d "sound\dialog\levels\01_spacestation\mission\l01_0100_mm1"))
        (print "(grunts and snarls)")
        (print "check your targets, watch the crossfire!")
        (sound_impulse_time "sound\dialog\levels\01_spacestation\mission\l01_0120_mm1" none 1.0 "radio_default")
        (sleep (- (sound_impulse_start_3d "sound\dialog\levels\01_spacestation\mission\l01_0120_mm1") 60.0))
        (object_create_anew "1st_craft")
        (device_set_position_immediate "1st_craft" 1.0)
        (device_set_position "1st_craft" 0.0)
        (sleep 65)
        (print "they're in standard formation: little bastards up front, big ones in back. good luck, cairo!")
        (sound_impulse_time "sound\dialog\levels\01_spacestation\mission\l01_0130_mm1" none 1.0 "radio_default")
        (sleep_until (< (device_get_position "1st_craft") 0.75) 1)
        (print "docking sound")
        (sound_impulse_start_effect "sound_remastered\ambience\spacestation\docking_clangs\dockingcov_incoming" none 1.0)
        (sleep_until (<= (device_get_position "1st_craft") 0.1) 1)
        (print "klang malta!")
        (damage_players "effects\scenarios\objects\solo\spacestation\boarding_craft_shake")
