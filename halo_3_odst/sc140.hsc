(script static void start
    (if (not (campaign_survival_enabled)) 
        (wake sc140_mission))
    (cond
        ((= (game_insertion_point_get) 0)
            (ins_level_start)
        )
        ((= (game_insertion_point_get) 1)
            (ins_cell_1b)
        )
        ((= (game_insertion_point_get) 2)
