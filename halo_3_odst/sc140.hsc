                    )
                )
                ((and
                    (= banshee_rand_short 3)
                    (< (ai_living_count "banshee04") 1)
                    (< tempbanshee gnumber)
                )
                    (begin
                        (sleep 90)
                        (ai_place "banshee04")
                        (set tempbanshee (+ tempbanshee 1.0))
                    )
                )
                ((and
                    (= banshee_rand_short 4)
                    (< (ai_living_count "banshee05") 1)
                    (< tempbanshee gnumber)
                )
                    (begin
                        (sleep 90)
